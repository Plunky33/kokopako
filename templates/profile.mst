<div class="_section js-section">
    <div class="_animate js-viewport">
        <h1 class="_blaze js-split" data-split="lines, words, chars">{{ infos.blaze }}</h2>
        <h1 class="_name js-split" data-split="lines, words, chars">{{ infos.name }}</h1>
    </div>
    <div class="_scroll_icon">
        <div></div>
    </div>
    <div class="_hp-row row">
        <div class="column col-6">
            <div class="row">
                <div class="column col-8">
                    <div class="row vs-section vs-about">
                        <div class="_animate js-viewport">
                            <div class="_hr-row">
                                <hr class="_primary_bg">
                                <span class="_primary_color">Profile</span>
                            </div>
                        </div>
                        <div class="_animate js-viewport">
                            <h1 class="h1 js-split" data-split="lines">{{ infos.tagline }}</h1>
                        </div>
                        {{ #infos.description }}
                        <div class="_animate js-viewport">
                            <p class="js-split" data-split="lines">{{.}}</p>
                        </div>
                        {{ /infos.description }}
                        <div class="_animate js-viewport">
                            <div class="_hr-row">
                                <hr class="_primary_bg">
                                <span class="_primary_color">Agencies and Clients</span>
                            </div>
                        </div>
                        <div class="_animate js-viewport">
                            <h1 class="h2 js-split" data-split="lines">{{ infos.agencies_big }}</h1>
                        </div>
                        <div class="_animate js-viewport">
                            <p class="js-split" data-split="lines">{{ infos.agencies_small }}</p>
                        </div>
                        <div class="_animate js-viewport">
                            <div class="_hr-row">
                                <hr class="_primary_bg">
                                <span class="_primary_color">Contact</span>
                            </div>
                        </div>
                        <div class="_animate js-viewport" style="overflow: visible;">
                            <ul>
                                <a class="_a no-route" href="mailto:{{ infos.email }}">{{ infos.emailÂ }}</a>
                            </ul>
                        </div>
                        <div class="_animate js-viewport">
                            <small>{{{ infos.copyright }}}</small>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="column col-6">
            <div class="row">
                <div class="column col-8 vs-section vs-awards">
                    <div class="_block js-viewport">
                        <div class="_hr-row _hr-row--black">
                            <hr>
                        </div>
                        <h2 class="h4 js-split" data-split="lines, words, chars">Webby Awards x{{webby_awards.number}}</h2>
                        <ul>
                            {{#webby_awards.items}}
                            <li>
                                <div class="overflow">
                                    <span><a href="{{link}}" target="_blank" data-type="award">{{title}}</a></span>
                                </div>
                            </li>
                            {{/webby_awards.items}}
                        </ul>
                    </div>
                    <div class="_block js-viewport">
