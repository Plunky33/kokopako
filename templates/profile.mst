                            <div class="overflow">
                                <strong>Adobe the Cutting Edge</strong>
                            </div>
                            {{#fwa.ace}}
                            <li>
                                <div class="overflow">
                                    <span><a href="{{link}}" target="_blank" data-type="award">{{title}}</a></span>
                                </div>
                            </li>
